<div class="am-modal-dialog">
    <div class="am-modal-hd">新增目录
        <a href="javascript: void(0)" class="am-close am-close-spin" data-am-modal-close>&times;</a>
    </div>
    <div class="am-modal-bd">
        <form class="am-form am-form-horizontal" method="post" id="catalogue_new" v-on:submit.prevent="onSubmit">
            <fieldset>
                <div class="am-form-group">
                    <label for="reportId" class="am-u-sm-3">目录编号</label>
                    <div class="am-u-sm-9">
                        <input type="text" class="am-input-sm"  v-model="datas.F_id"
                               placeholder="请输入目录编号" disabled required>
                    </div>
                </div>
                <div class="am-form-group">
                    <label for="formType" class="am-u-sm-3">目录名称</label>
                    <div class="am-u-sm-9">
                        <input type="text" class="am-input-sm" v-model="datas.F_name"
                               placeholder="请输入目录名称" maxlength="10" data-foolish-msg="请输入目录名称！" required>
                    </div>
                </div>
                <div class="am-form-group">
                    <label for="doc-ipt-3" class="am-u-sm-3">目录图标</label>
                    <div class="am-u-sm-9">
                        <input type="text" class="am-input-sm" v-model="datas.F_icon" placeholder="请输入目录图标" maxlength="10" data-foolish-msg="请输入目录图标！" required>
                    </div>
                </div>
                <div class="am-form-group">
                    <label for="doc-ipt-4" class="am-u-sm-3">重点与否</label>
                    <div class="am-u-sm-9">
                        <select class=" am-input-sm"  v-model="datas.F_start" data-foolish-msg="请选择重点与否！"
                                required>
                            <option value="0">非重点目录</option>
                            <option value="1">重点目录</option>
                        </select>
                    </div>
                </div>
                <div class="am-form-group">
                    <label for="doc-ipt-4" class="am-u-sm-3">父级</label>
                    <div class="am-u-sm-9">
                        <select class=" am-input-sm"  v-model="datas.F_parent" data-foolish-msg="请选择父级！"
                                >
                            <option value="0">非重点目录</option>
                            <option value="1">重点目录</option>
                        </select>
                    </div>
                </div>
                <div class="am-form-group">
                    <label for="doc-ipt-4" class="am-u-sm-3">允许删除</label>
                    <div class="am-u-sm-9">
                        <select class=" am-input-sm"  v-model="datas.F_AllowDelete" data-foolish-msg="请选择是否删除！"
                                required>
                            <option value="0">否</option>
                            <option value="1">是</option>
                        </select>
                    </div>
                </div>
                <div class="am-form-group">
                    <label for="doc-ipt-3" class="am-u-sm-3">链接地址</label>
                    <div class="am-u-sm-9">
                        <input type="text" class="am-input-sm" v-model="datas.F_url" placeholder="请输入链接地址" maxlength="100" data-foolish-msg="请输入链接地址！" required>
                    </div>
                </div>
                <div class="am-form-group">
                    <label for="reportStatistics" class="am-u-sm-3">备注</label>
                    <div class="am-u-sm-9">
                        <textarea rows="3" placeholder="请输入备注" maxlength="200" v-model="datas.F_remark"></textarea>
                    </div>
                </div>
                <div class="am-form-group">
                    <label class="am-u-sm-3">排序号</label>
                    <div class="am-u-sm-9">
                        <input type="text" class="am-input-sm"  v-model="datas.F_sort" placeholder="请输入排序号" data-foolish-msg="请输入排序号！" required>
                    </div>
                </div>
                <div class="am-form-group">
                    <div class="am-u-sm-10 am-u-sm-offset-2">
                        <button type="submit" class="am-btn am-btn-secondary am-radius" data-am-modal-close>取消</button>
                        <button class="am-btn am-btn-secondary" type="submit" v-on:click="saveDatas('doc-modal-1')" >提交</button>
                    </div>
                </div>
            </fieldset>
        </form>
    </div>
</div>